{{- range .Aliases }}
{{ printf "%s %s %d" (cond (hasPrefix . "/") . (path.Join $p.RelPermalink .)) $p.RelPermalink ($p.Params.HTTPCode | default 301) }}
{{- end }}
{{- end }}
